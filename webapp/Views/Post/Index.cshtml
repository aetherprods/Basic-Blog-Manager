@using shared_classes.Models
@model IEnumerable<PostModel>

<div class="row">
    <div class="col-md-10 col-md-offset-2">
        <table class="table">
            <tr>
                <th>Title</th>
                <th>Text</th>
                <th>Date Posted</th>
            </tr>
            @Html.DisplayForModel()
        </table>
        <a asp-action="Add" asp-route-blogId="@ViewBag.BlogID">Add</a>
    </div>
</div>